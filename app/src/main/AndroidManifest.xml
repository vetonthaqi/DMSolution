<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="co.dm.solution">

    <uses-permission android:name="android.permission.BLUETOOTH" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="co.dm.solution.RegisterActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="co.dm.solution.SpeachActivity" />
        <activity android:name="co.dm.solution.SignsActivity" />
        <activity android:name="co.dm.solution.AlphabetActivity" />
        <activity android:name="co.dm.solution.TextToSpeachActivity" />
        <activity android:name="co.dm.solution.SignsAlphabetActivity" />
        <activity android:name="co.dm.solution.SignsWordsActivity" />
        <activity android:name="co.dm.solution.PopUpActivity" />
        <activity android:name="co.dm.solution.LoginActivity" />
        <activity android:name="co.dm.solution.MainActivity" />
        <activity android:name="co.dm.solution.DeviceListActivity"></activity>
    </application>

</manifest>